#!/usr/bin/env ruby

require_relative '../lib/move.rb'

# Code your CLI Here
# Create board array
board = Array.new(9," ")

#Greet user
puts "Welcome to Tic Tac Toe!"

#Request input from user and convert to index number
user_input = gets.strip
user_input = input_to_index(user_input)

#Call move method
move(board,user_input)
